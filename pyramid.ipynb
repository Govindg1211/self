   "source": [
    "def print_pyramid(height):\n",
    "    for i in range(height):\n",
    "        # Print spaces for alignment\n",
    "        print(' ' * (height - i - 1), end='')\n",
    "        # Print stars\n",
    "        print('*' * (2 * i + 1))\n",
    "\n",
    "# Set the height of the pyramid\n",
    "height = 5\n",
    "print_pyramid(height)"
   ]
